    grDevices,
    methods,
    minpack.lm (>= 1.1-9),
    Rmpfr (>= 0.5-7),
    sp (>= 1.2-0),
    stats
Suggests:
    rgeos,
    rgdal,
    rgbif (>= 0.9.2),
    knitr,
    rmarkdown,
    bookdown
LazyData: true
